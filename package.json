{
  "name": "@ajtii/commons",
  "version": "0.1.0-alpha",
  "description": "Reusable components",
  "keywords": [
    "ajtii",
    "commons"
  ],
  "homepage": "https://github.com/ajtii-com/commons#readme",
  "bugs": {
    "url": "https://github.com/ajtii-com/commons/issues"
  },
  "license": "MIT",
  "author": "Ajtii Team <ajtii.com@gmail.com>",
  "contributors": [
    "David Bojnansky <david.bojnansky@gmail.com>"
  ],
  "files": [
    "dist/",
    "!dist/**/*.spec.js",
    "!dist/**/*.spec.js.map",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "repository": "github:ajtii-com/commons",
  "scripts": {
    "build": "rimraf dist/ && tsc -p tsconfig.build.json && copyfiles -u 2 src/types/**/*.d.ts dist/types/",
    "test": "yarn build && tsc -p tsconfig.test.json && cross-env NODE_ENV=testing   mocha --opts mocha.opts",
    "test-cov": "nyc   yarn test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.9.4",
    "@types/sequelize": "^4.27.7",
    "@types/sinon": "^4.1.3",
    "@types/verror": "^1.10.3",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.3",
    "javascript-stringify": "^1.6.0",
    "mocha": "^5.0.3",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.3.0",
    "source-map-support": "^0.5.3",
    "typescript": "^2.7.2",
    "verror": "^1.10.0",
    "yarn": "^1.5.1"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "types": "dist/types/index.d.ts",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js"
    ],
    "cache": true,
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
